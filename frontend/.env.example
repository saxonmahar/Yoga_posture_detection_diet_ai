# ===================================
# YOGA AI - FRONTEND ENVIRONMENT VARIABLES
# ===================================
# Copy this file to .env and fill in your actual values

# ===== API CONFIGURATION =====
# Backend API URL (no trailing slash)
VITE_API_URL=http://localhost:5001

# ML Service URL
VITE_ML_API_URL=http://localhost:5000

# Diet Service URL
VITE_DIET_API_URL=http://localhost:5002

# ===== APPLICATION SETTINGS =====
VITE_APP_NAME=YogaAI
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=AI-Powered Yoga Posture Detection & Diet Recommendation

# ===== DEVELOPMENT SETTINGS =====
VITE_NODE_ENV=development
VITE_DEBUG=true

# ===== OPTIONAL SERVICES =====
# Add any frontend-specific configurations here
VITE_GOOGLE_ANALYTICS_ID=your_ga_id_here
VITE_SENTRY_DSN=your_sentry_dsn_here
VITE_STRIPE_PUBLIC_KEY=your_stripe_public_key